{
  "name": "pet-adoption",
  "version": "1.0.0",
  "description": "pet adoption helper",
  "main": "src/index.html",
  "scripts": {
    "build:client": "parcel build --public-url ./dist/ src/index.html",
    "build:server": "parcel build -d dist-server --target node server/index.js",
    "build": "npm run build:client && npm run:server",
    "start": "npm -s run build && node dist-server/index.js",
    "dev": "parcel src/index.html",
    "format": "prettier --write \"src/**/*.{js, jsx}\"",
    "lint": "eslint \"src/**/*.{js, jsx}\" --quiet"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DidiGain/pet-adoption.git"
  },
  "keywords": [
    "pet",
    "adopt"
  ],
  "author": "Dinara Gainulina",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DidiGain/pet-adoption/issues"
  },
  "homepage": "https://github.com/DidiGain/pet-adoption#readme",
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/eslint-parser": "^7.14.7",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "parcel": "*",
    "prettier": "^2.3.2"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ]
}
